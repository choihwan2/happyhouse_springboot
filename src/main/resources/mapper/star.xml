<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.stardao">

	<select id="getStarApt" parameterType="string" resultType="housedeal">
		SELECT gugun, name, address, weekdayop, tel, lat, lng, type FROM hospital WHERE gugun_code like concat(#{gugun}, '%')
	</select>
	
	<insert id="saveStarApt" parameterType="starapt">
		INSERT INTO
		starapt(userId,aptNo) VALUES(#{userId},#{aptNo})
	</insert>
	
</mapper>